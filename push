#!/bin/bash

## example usage:
## bash push "commit message"

rsync -arvuP --delete --exclude-from=excluded ~/.emacs.d/ .emacs.d/
rsync -avuP ~/.emacs .
git add .
if [ -z $1 ]
then
    git commit --all -m $0
else
    git commit --all -m $1
fi
git push -u origin master
# # git push git@github.com:stakahama/dot-emacs.git master